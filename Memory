v0x7fed08419f80_0 .var "r", 15 0;
E_0x7fed08404d80 .event edge, v0x7fed084088b0_0;
    .scope S_0x7fed08406550;
T_0 ;
    %set/v v0x7fed08419e80_0, 0, 32;
T_0.0 ;
    %load/v 8, v0x7fed08419e80_0, 32;
    %movi 40, 65536, 32;
    %cmp/s 8, 40, 32;
    %jmp/0xz T_0.1, 5;
    %ix/getv/s 3, v0x7fed08419e80_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x7fed08419f00, 0, 16;
t_0 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x7fed08419e80_0, 32;
    %set/v v0x7fed08419e80_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
    .scope S_0x7fed08406550;
T_1 ;
    %wait E_0x7fed08404d80;
    %ix/getv 3, v0x7fed084088b0_0;
    %load/av 8, v0x7fed08419f00, 16;
    %set/v v0x7fed08419f80_0, 8, 16;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x7fed08406470;
T_2 ;
    %vpi_call 2 9 "$dumpfile", "Memory.vcd";
    %vpi_call 2 10 "$dumpvars", 1'sb0, S_0x7fed08406470;
    %vpi_call 2 11 "$monitor", "%t: a = %b, out = %b", $time, v0x7fed0841a040_0, v0x7fed0841a0d0_0;
    %movi 8, 54, 16;
    %set/v v0x7fed0841a040_0, 8, 16;
    %delay 10, 0;
    %movi 8, 37, 16;
    %set/v v0x7fed0841a040_0, 8, 16;
